overhead "[+Start ProvoNearest]" 111
clearall 

hotkey 'target closest non-friendly monster'
wait 200
if insysmsg "no one matching"
    overhead  "Nothing to attack" 34
    stop
endif

@setvar! mobOne lasttarget 
getlabel mobOne descOne
overhead "MobOne = {{descOne}}" 191
wait 250

hotkey 'next target non friendly'
wait 200
if insysmsg "no one matching"
    overhead  "Nothing to attack" 34
    stop
endif

@setvar! mobTwo lasttarget
getlabel mobTwo descTwo
overhead "MobTwo = {{descTwo}}" 191
wait 250

useskill 'Provocation'
wft 250
target mobOne
wft 500
target mobTwo

hotkey 'clear target queue'