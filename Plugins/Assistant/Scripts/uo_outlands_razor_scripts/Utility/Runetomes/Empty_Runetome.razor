overhead "Select a runetome to empty all runes from" 88
@setvar! dump_runetome_helper
dclick dump_runetome_helper
waitforgump 167090027 1000
gumpresponse 200
waitforgump 167090027 1000
for 28
    clearsysmsg 
    wait 500
    gumpresponse 15
    waitforgump 167090027 1000
    wait 500
    if not insysmsg "You remove the rune from the rune tome"
        break
    endif
endfor
overhead "All done"

while findtype "recall rune" backpack 0
    wait 1000
    lifttype "recall rune" 1 backpack 0
    droprelloc 0 0
endwhile
