if findtype 3702 ground -1 -1 2 as bag
    dclick bag  
    wait 500 
    foreach x in ItemTypes
        if findtype x bag as found
            hotkey 'set last target'
            target found
            @setvar myVariable lasttarget
            wait 200
            getlabel 'myVariable' myLabel
            if 'unidentified' in myLabel
                skill 'itemidentification'
                waitfortarget
                target myVariable
                wait 425
            endif
            sysmsg myLabel 85
            wait 600
        endif
        ignore x
    endfor
endif

if findtype 3702 ground -1 -1 2 as bag
    dclick bag  
    wait 500 
    foreach x in ItemTypes
        if findtype x bag as found
            hotkey 'set last target'
            target found
            @setvar myVariable lasttarget
            wait 200
            getlabel 'myVariable' myLabel
            if 'defense' in myLabel
                lift found
                wait 500
                drop backpack
            elseif 'guarding' in myLabel
                lift found
                wait 500
                drop backpack
            elseif 'ruin' in myLabel
                lift found
                wait 500
                drop backpack
            elseif 'melodious' in myLabel
                lift found
                wait 500
                drop backpack
            endif               
        endif
    endfor
endif 

sysmsg 'Done'