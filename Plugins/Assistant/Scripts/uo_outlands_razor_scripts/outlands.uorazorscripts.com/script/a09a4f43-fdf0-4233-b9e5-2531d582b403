hotkey 'use item in hand'
wft 300
if insysmsg "What do you want"
    waitfortarget 
    target self
    wft 500
    if insysmsg "You do not see any"
        overhead 'Spot Done' 33
        sysmsg 'Spot Done' 33
        clearsysmsg 
    break
    endif
    wft 3000
endif
wft 1000
if insysmsg "You do not see any"
    overhead 'Spot Done' 33
    sysmsg 'Spot Done' 33
    clearsysmsg 
break
endif
wait 300
 if not insysmsg "skill gain" and not insysmsg "You do not see any" and not insysmsg 'You hack' and not insysmsg 'You chop' and not insysmsg 'world  is saving' and not insysmsg 'world will save' and not insysmsg 'World save complete' and not insysmsg 'You dig' and not insysmsg 'You loosen' and not insysmsg 'Distance to destination' and not insysmsg  'Now tracking'
    wait 300
    if not insysmsg "skill gain" and not insysmsg "You do not see any" and not insysmsg 'You hack' and not insysmsg 'You chop' and not insysmsg 'world  is saving' and not insysmsg 'world will save' and not insysmsg 'World save complete' and not insysmsg 'You dig' and not insysmsg 'You loosen' and not insysmsg 'Distance to destination' and not insysmsg  'Now tracking'
                overhead 'Captcha break' 34
    break
    endif
endif
replay

