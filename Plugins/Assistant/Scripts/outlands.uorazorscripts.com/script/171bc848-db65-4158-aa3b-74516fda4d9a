while findtype 'map' 'backpack' as tmap
    getlabel tmap itemLabel
    overhead itemLabel
    lift tmap
    wait 250
    if 'level 1' in itemLabel or 'fishing map' in itemLabel
        if poplist t1Cords 'front' as yy
            drop container_target yy 70 0
        else
            overhead "t1 empty!"
            stop
        endif
    elseif 'level 2' in itemLabel or 'skinning map' in itemLabel
        if poplist t2Cords 'front' as yy
            drop container_target yy 90 0
        else
            overhead "t2 empty!"
            stop
        endif
    elseif 'level 3' in itemLabel or 'lumber map' in itemLabel
        if poplist t3Cords 'front' as yy
            drop container_target yy 110 0
        else
            overhead "t3 empty!"
            stop
        endif
    elseif 'level 4' in itemLabel or 'ore map' in itemLabel
        if poplist t4Cords 'front' as yy
            drop container_target yy 130 0
        else
            overhead "t4 empty!"
            stop
        endif
    elseif 'level 5' in itemLabel
        if poplist t5Cords 'front' as yy
            drop container_target yy 90 0
        else
            overhead "t5 empty!"
            stop
        endif
    elseif 'level 6' in itemLabel
        if poplist t6Cords 'front' as yy
            drop container_target yy 110 0
        else
            overhead "t5 empty!"
            stop
        endif
    else
        overhead "Something wrong!" 32
        stop
    endif
    ignore tmap
endwhile
overhead "Out of list!"