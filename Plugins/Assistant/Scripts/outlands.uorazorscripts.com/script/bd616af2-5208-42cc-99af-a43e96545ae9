# Drop a stool by Jaseowns
# UO Outlands
if findtype "stool" backpack as chair
    lift chair
    droprelloc 0 0
endif
wait 1000
if findtype "stool" ground -1 -1 2 as chair
    lift chair
    drop backpack
endif
