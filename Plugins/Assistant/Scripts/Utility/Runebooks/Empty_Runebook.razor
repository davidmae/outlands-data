overhead "Select a runebook to empty all runes from" 88
@setvar! dump_runebook_helper
dclick dump_runebook_helper
waitforgump 1551740969 1000
for 16
    clearsysmsg 
    wait 500
    gumpresponse 3
    waitforgump 1551740969 1000
    if not insysmsg "You have removed the rune"
        break
    endif
endfor
overhead "All done"

while findtype "recall rune" backpack 0
    wait 1000
    lifttype "recall rune" 1 backpack 0
    droprelloc 0 0
endwhile
