clearsysmsg 

if not listexists 'vettimer'
    createlist 'vettimer'
endif

if findtype 25750 backpack
    dclicktype 25750 backpack
    wait 200
    if insysmsg 'living followers'
        overhead 'Vet: Nothing to heal' 88
    else 
        if insysmsg "You must wait a few moments"
            overhead 'Vet: already running' 88
        else 
            clearlist 'vettimer'
            pushlist 'vettimer' '1'
            pushlist 'vettimer' '2'
            pushlist 'vettimer' '3'
            pushlist 'vettimer' '4'
            pushlist 'vettimer' '5'
            
            if dex < 100
                pushlist 'vettimer' '6'
                pushlist 'vettimer' '7'
            endif
            
            overhead 'Vet: Starting' 88
        endif
        
        if list "vettimer" = 7
            overhead 'Vet: 7s' 88
            poplist 'vettimer' 'front'
            wait 1000
        endif
        
        if list "vettimer" = 6
            overhead 'Vet: 6s' 88
            poplist 'vettimer' 'front'
            wait 1000
        endif
        
        if list "vettimer" = 5
            overhead 'Vet: 5s' 88
            poplist 'vettimer' 'front'
            wait 1000
        endif
        
        if list "vettimer" = 4
            overhead 'Vet: 4s' 88
            poplist 'vettimer' 'front'
            wait 1000
        endif
        
        if list "vettimer" = 3
            overhead 'Vet: 3s' 88
            poplist 'vettimer' 'front'
            wait 1000
        endif
        
        if list "vettimer" = 2
            overhead 'Vet: 2s' 88
            poplist 'vettimer' 'front'
            wait 1000
        endif
        
        if list "vettimer" = 1
            overhead 'Vet: 1s' 88
            poplist 'vettimer' 'front'
            wait 500
        endif
        
        
        overhead 'Vet: End' 88
        
            
        
    endif
else 
    overhead 'No Supplies!' 34
endif