if not findtype "keg" backpack
    restock 1
    wft
    target "restock_container_regular_kegs"
    wait 2500
endif

if findtype "keg" backpack
    dclicktype "keg" backpack
    wait 500
else
    overhead "No keg found!" 34
    wait 2500
    replay
endif

if insysmsg "The keg is empty"
    organizer 1
    wait 2500
    replay
endif

if hp > 50
    if findtype "Green Potion" backpack
        skill 'poisoning'
        waitfortarget
        targettype "Green Potion" backpack
        waitfortarget
        targettype "kryss" backpack
        wait 3500
    else
        overhead "No potion found!" 34
    endif
else
    overhead "You might die..." 34
endif

if findtype "Yellow Potion" backpack and hp < 50
    dclicktype "Yellow Potion"
    wait 500
endif

while poisoned 

    if findtype "Orange Potion" backpack
        dclicktype "Orange Potion" backpack
        wait 500
    endif
    
    if findtype "Yellow Potion" backpack and hp < 50
        dclicktype "Yellow Potion"
        wait 500
    endif
endwhile












loop